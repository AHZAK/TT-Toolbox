function [tt1]=ctranspose(tt)
%[TT1]=CTRANSPOSE(TT)
%Compute complex conjugate transpose of a TT-matrix


t0=tt.tt;
d=t0.d;
n=tt.n;
m=tt.m;
r=t0.r;
ps=t0.ps;
cr=t0.core;
tt1=t0;
for i=1:d
core1=cr(ps(i):ps(i+1)-1);
core1=reshape(core1,[r(i),n(i),m(i),r(i+1)]);
core1=conj(core1);
core1=permute(core1,[1,3,2,4]);
cr(ps(i):ps(i+1)-1)=core1;
end
tt1.core=cr;

%Zaglushka
%tt1=tt;
%tt1.tt=tt_tensor(tt_mat_to_vec(tt_transp(tt_vec_to_mat(core(tt.tt),tt.n,tt.m))));
%keyboard