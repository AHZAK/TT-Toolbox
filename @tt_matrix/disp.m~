function disp(tt,name)
%DISP Command window display of a tt_tensor.
%

if ~exist('name','var')
    name = 'ans';
end
tt1=tt.tt;
r=tt1.r; n=tt.n; m=tt.m;
d=tt1.d;
fprintf('%s is a %d-dimensional TT-matrix, ranks and mode sizes: \n',name,d);
%fprintf('r(1)=%d \n', r(1));
tbl=cell(d+1,3);
for i=1:d
  tbl{i,1}=sprintf('r(%d)=%d',i,r(i));
  tbl{i,2}=sprintf('n(%d)=%d',i,n(i));
  tbl{i,3}=sprintf('m(%d)=%d',i,m(i));
end
tbl{d+1,1}=sprintf('r(%d)=%d',d+1,r(d+1));
%for i=1:d
%   fprintf('r(%d)=%d \t n(%d)=%d m(%d) = %d \n',i,r(i),i,n(i),i,m(i));
%end
fprintf('r(%d)=%d \n',d+1,r(d+1));

